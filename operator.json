{
  "name": "Logit",
  "description": "Transforms a binary outcome to a continuous variable.",
  "tags": [
    "preprocessing",
    "transformation"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/rdewijn/logit_operator"
  ],
  "properties": [
    {
      "kind": "BooleanProperty",
      "name": "pct",
      "defaultValue": false,
      "description": "Logical value that indicates if the proportion are expressed as percentages."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "Values",
                "type": "numeric",
                "description": "Values to calculate logit transform on.",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}